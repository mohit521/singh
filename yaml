# application.yml
spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/clothing_store
    username: store_user
    password: strongpassword
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate.format_sql: true
server:
  port: 8080
jwt:
  secret: YOUR_256_BIT_SECRET
  expirationMs: 86400000
